$('.secondButton').css("display", "block");
}, function() {
$('.textSecond').css({"color": "rgb(33, 49, 77)" , "z-index": "-1", "background-color":"white", "position": "initial" });
$('.secondButton').css("display", "none");
$('.divSecond').css({ "z-index": "1" });
});
$(".divThird").hover(function() {
$('.textThird').css({"color": "white" , "z-index": "2", "background":"rgba(33, 49, 77, 0.5)", "position": "relative" });
$('.thirdButton').css("display", "block");
}, function() {
$('.textThird').css({"color": "rgb(33, 49, 77)" , "z-index": "-1", "background-color":"white", "position": "initial" });
$('.thirdButton').css("display", "none");
$('.divThird').css({ "z-index": "1" });
});
