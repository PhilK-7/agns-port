<script>
    (function () {
        PX_MWC.Config.language = 'nl';
        PX_MWC.Config.contentWallUrl= '/pickx/nl/contentwall';
        PX_MWC.Config.subscriptionsUrl= '/pickx/nl/abonnementen';

        PX_MWC.Config.refreshTokenCallBack = function () {

            if (!PX_MWC.Config.getOauthToken()) {
                window.location = '/pickx/nl/connect/start?ru=' + encodeURIComponent(window.location.href);
                return;
            }

            var url = "https://www.proximus.be/pickx/nl/connect/refresh?ru=" + encodeURIComponent(window.location.href);
            const refreshTokenPromise = fetch(url, {
                credentials: "include"
            });

            return new Promise(function(resolve, reject) {
                setTimeout(function() {
                    reject(new Error("Timeout when contacting trying to refresh the token"));
                }, 20000);

                return refreshTokenPromise.then(resolve, reject);
            }).then( function(response) {
                if (response.status === 200){
                    return response.json();
                }

                window.location = '/pickx/nl/connect/start?ru=' + encodeURIComponent(window.location.href);
            }).then ( function(token) {
                return token.accessToken;
            }).catch( function(error) {
                console.error('Could not refresh access token.', error);
            });
        };
        PX_MWC.Config.registrationCallback = function () {
            window.location = '/pickx/nl/connect/select-account';
        };

        function slugify(string) {
            const a = 'àáâäæãåāăąçćčđďèéêëēėęěğǵḧîïíīįìłḿñńǹňôöòóœøōõṕŕřßśšşșťțûüùúūǘůűųẃẍÿýžźż·/_,:;';
            const b = 'aaaaaaaaaacccddeeeeeeeegghiiiiiilmnnnnooooooooprrsssssttuuuuuuuuuwxyyzzz------';
            const p = new RegExp(a.split('').join('|'), 'g')

            return string.toString().toLowerCase()
                .replace(/\s+/g, '-') // Replace spaces with -
                .replace(p, function (c) { return b.charAt(a.indexOf(c)) }) // Replace special characters
                //.replace(/&/g, '-and-') // Replace & with 'and'
                .replace(/[^\w\-]+/g, '') // Remove all non-word characters
                .replace(/\-\-+/g, '-') // Replace multiple - with single -
                .replace(/^-+/, '') // Trim - from start of text
                .replace(/-+$/, '') // Trim - from end of text
        }

        PX_MWC.Config.detailsUrl = function (id, title, type) {
                            if (type === 'tvod'){
                    var url = '/pickx/nl/televisie/proximus-vod/' + id + '/' + slugify(title);
                } else {
                    var url = '/pickx/nl/televisie/tv-gids/' + id + '/' + slugify(title);
                }
            
            if (!Pickx.singlePage.isEnabled()) {
                document.location = url;

                return;
            }

            Pickx.singlePage.setFocusedElementBeforeOverlay(document.activeElement);
            Pickx.singlePage.go(url);
        };

        PX_MWC.Config.loginRedirect = function(vodId) {
            if (!history.state || !history.state.previous.url) {
                window.location = '/pickx/nl/connect/start?ru=' + encodeURIComponent(window.location.href);
                return;
            }

            window.location = '/pickx/nl/connect/start?ru=' + encodeURIComponent(history.state.previous.url + '#vod:' + vodId);
        };

                PX_MWC.Config = Object.assign(PX_MWC.Config, {"serviceUrl":{"authenticated":"https:\/\/www.proximus.be\/MWC\/api\/v2\/graphql","authenticatedV3":"https:\/\/www.proximus.be\/MWC\/api\/v3\/graphql","anonymous":"https:\/\/api.proximusmwc.be\/v2\/graphql","anonymousV3":"https:\/\/api.proximusmwc.be\/v3\/graphql"}});
            })();
</script>

